{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "shell",
			"args": [
				"build"
			],
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"isBackground": false
		},
		{
			"label": "run",
			"command": "dotnet",
			"type": "shell",
			"args": [
				"run",
				"--project",
				"CleanArchitecture.Web"
			],
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"isBackground": true
		},
		{
			"label": "watch",
			"command": "dotnet",
			"type": "shell",
			"args": [
				"watch",
				"run",
				"--project",
				"CleanArchitecture.Web"
			],
			"group": "build",
			"problemMatcher": [
				"$msCompile"
			],
			"isBackground": true
		}
	]
}